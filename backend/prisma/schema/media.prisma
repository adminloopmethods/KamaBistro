model Media {
  id         String    @id @default(uuid())
  url        String
  publicId   String
  type       MediaType
  width      Int?
  height     Int?
  altText    Json? 
  createdAt  DateTime  @default(now())
  // Relation
  // resource   Resource  @relation(fields: [resourceId], references: [id], onDelete: Cascade)
  resourceId String

  @@index([resourceId])
}
